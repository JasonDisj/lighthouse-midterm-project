<!DOCTYPE html>
<html lang="en">

<head>
  <title>Favourites</title>

  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">


  <link href="http://fonts.cdnfonts.com/css/new-super-mario-font-u" rel="stylesheet">
  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" />
  <link rel="stylesheet" href="/vendor/border-box.css" />
  <link rel="stylesheet" href="/styles/main.css" />
  <link rel="stylesheet" href="/styles/layout.css" />
  <link rel="stylesheet" href="/styles/favourite-page.css" />
  <link rel="stylesheet" href="/styles/nav.css" />

  <script src="/vendor/jquery-3.0.0.js"></script>
  <script defer src="/scripts/users.js"></script>
</head>

<body>

  <nav>
    <div id="navlogo">
      <span><a href="http://localhost:8080"><img
            src="https://fontmeme.com/permalink/220827/af0c2ad64ef606ff785badeebe948d07.png"></a></span>
    </div>

  </nav>

  <h1 class="favourite-page">Favourite video games</h1>

  <% for (let item of favs) { %>
    <div class="new-games">
      <div>
        <h3>
          <%= item.name %>
        </h3>
      </div>
      <div><img class="game-1-pic" src='<%= item.cover_photo_url %>' </div>
        <div>
          <h3>$ <%= item.price %>
          </h3>
        </div>
        <div>
          <p>
            <%= item.description %>
          </p>
        </div>
      </div>

      <% } %>

</body>

</html>
